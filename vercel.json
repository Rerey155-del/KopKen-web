{
  "builds": [
    {
      "src": "Server/koneksi.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Server/koneksi.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.jsx"
    }
  ]
}
