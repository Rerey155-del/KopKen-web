{
  "builds": [
    {
      "src": "Server/koneksi.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Server/koneksi.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}